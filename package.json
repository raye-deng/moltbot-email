{
  "name": "moltbot-email",
  "version": "1.0.0",
  "description": "Email (Gmail) channel plugin for Moltbot",
  "main": "index.ts",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/raye-deng/moltbot-email.git"
  },
  "homepage": "https://github.com/raye-deng/moltbot-email#readme",
  "bugs": {
    "url": "https://github.com/raye-deng/moltbot-email/issues"
  },
  "keywords": [
    "moltbot",
    "clawdbot",
    "email",
    "gmail",
    "chat",
    "bot"
  ],
  "files": [
    "src",
    "index.ts",
    "moltbot.plugin.json",
    "README.md"
  ],
  "clawdbot": {
    "extensions": [
      "./index.ts"
    ],
    "channel": {
      "id": "email",
      "label": "Email",
      "selectionLabel": "Email (Gmail)",
      "detailLabel": "Email",
      "docsPath": "/channels/email",
      "docsLabel": "email",
      "blurb": "Email channel with Gmail support.",
      "aliases": [
        "gmail",
        "mail"
      ],
      "order": 70
    },
    "install": {
      "npmSpec": "moltbot-email",
      "defaultChoice": "path"
    }
  },
  "author": "Raye Deng",
  "license": "MIT",
  "dependencies": {
    "googleapis": "^144.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "clawdbot": "2026.1.24-3",
    "typescript": "^5.7.3",
    "vitest": "^3.0.2"
  },
  "peerDependencies": {
    "clawdbot": ">=2026.1.24"
  }
}
